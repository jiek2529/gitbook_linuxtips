        "Jiek's Blog": "http://jiek.icitsoft.com"
    }
},
"title": "Jiek's GitBook",
"variables": {},
"language": "zh-hans",
"plugins": [
    "anchors",
    "anchor-navigation-ex",
    "splitter",
    "expandable-chapters",
    "-lunr",
    "-search",
    "search-pro",
    "facebook",
    "-livereload",
    "theme-api",
    
    "fbqx",
    "advanced-emoji",
    "include-codeblock", 
    "emphasize",
    "ace",
    "mcqx"
],
"pluginsConfig": {
    "facebook": {
        "appKey": "206811166450251"
        },
    "include-codeblock": {
        "template": "ace"
        },
    "fontsettings": {
            "theme": "white",
            "family": "serif",
            "size": 2
        },
    "theme-api": {
      "languages": [
        {
          "lang": "js",          // sample lang argument
          "name": "JavaScript",  // corresponding name to be displayed
          "default": true        // default language to show
        },
        {
          "lang": "go",
          "name": "Go"
        }
      ]}
    }
